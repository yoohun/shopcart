.detail{
  margin-bottom: 100rpx;
  box-sizing: border-box 
}
.aaaa{
  height: 100rpx;
  width: 10rpx;
}
.imgScroll{
  width: 100%;
}
.slide-image{
  width: 100%;
}
swiper{
  height: 200rpx;
}
.swiper image {
  width: 100%;
  height: auto;
}
.product{
  width: 100%;
  padding: 10rpx 0;
}
.title{
  width: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;
  word-wrap:break-word; 
  word-break:normal;
  overflow: hidden;
  text-overflow:ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  font-size: 34rpx;
  line-height: 48rpx;
}
.product .productPAN{
  display: flex;
  justify-content: space-between
}
.product .productPAN .price{
  color: red;
  padding: 0 20rpx;
  box-sizing: border-box;
  font-size: 36rpx;
  margin: 20rpx 0
}
.product .productPAN .productName{
  width: 70%;
  padding: 0 8rpx;
  margin-right: 20rpx;
  box-sizing: border-box;
  display: inline-block;
  color: black;
  font-size: 34rpx;
  margin: 20rpx 0;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  text-align: right;
}
.product .message{
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  color: rgb(172, 172, 172);
  padding: 0 20rpx;
  box-sizing: border-box;
}
.productDetail{
  margin-top: 30rpx;
}
.productDetail .xq{
  font-size: 36rpx;
  font-weight: bold;
  margin-top: 16rpx;
  display: inline-block;
  line-height: 80rpx;
  margin-left: 20rpx;
}
.detailImage{
  width: 100%;
}
.detailImage image{
  width: 100%;
}
.recommend{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  background: rgb(245, 244, 244);
  margin-bottom: 90rpx;
}
.recommendItem{
  width: 47%;
  box-sizing: border-box;
  /* margin-bottom: 10rpx; */
  margin: 0 1.5% 40rpx;
  border: 1rpx solid gainsboro;
  border-radius: 10rpx;
  background: white;
  overflow:hidden;
}
.recommendImag{
  width: 100%;
}
.recommendImag image{
  width: 100%;
}
.recommendItem .title{
  font-size: 26rpx;
  display: block;
  line-height: 44rpx;
  width: 100%;
  padding: 0 12rpx;
  box-sizing: border-box;
  word-wrap:break-word; 
  word-break:normal;
  overflow: hidden;
  text-overflow:ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin: 14rpx 0;
}
.recommendText{
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  color: rgb(173, 172, 172);
  box-sizing: border-box;
  padding: 0 12rpx;
  margin-bottom: 12rpx;
}
.recommend .look{
  margin: 10rpx auto 30rpx;
  text-align: center;
  font-size: 30rpx;
  color: gray;
}
.recommendText text{
  display: inline-block;
  line-height: 50rpx;
}
.recommendText text:first-child{
  color: orangered;
}
.bottomRow .left{
  height: 90rpx;
  width: 120rpx;
  line-height: 90rpx;
  text-align: center;
  color: rgb(129, 129, 129);
  float: left
}
.bottomRow .borderRight{
  border-right: 1rpx solid rgb(204, 204, 204)
}
.bottomRow .right{
  flex: 1;
  color: white;
  text-align: center;
  height: 90rpx;
}
.bottomRow .right .item{
  width: 50%;
  float: left;
  height: 100%;
  line-height: 90rpx;
}
.intoCar{
  background: rgb(0, 182, 0)
}
.buyIt{
background: rgba(248, 19, 19, 0.774)
}
.bottomRow .left .iconfont{
  color: rgb(173, 173, 173);
  font-size: 40rpx
}
.buyList{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  font-size: 30rpx;
  z-index: 10;
  background: white;
  height: 90rpx;
  display: flex;
}
.choseCount{
  position: fixed;
  left: 0;
  right: 0;
  bottom:-440rpx;
  width: 100%;
  background: white;
  z-index: 99
}
.count{
  width: 100%;
  border-bottom: 1rpx solid rgb(172, 172, 172);
  border-top: 1rpx solid rgb(172, 172, 172);
  line-height: 50rpx;
  padding: 20rpx 20rpx;
  margin-bottom: 20rpx;
  font-size: 34rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center
}
.count>text{
  line-height: 90rpx;
  flex: 1;
}

.sureBtn{
  color: white;
  border-radius: 0
}
.burDes{
  display: flex;
  padding: 0rpx 20rpx 20rpx;
  border-top: 1rpx solid rgb(202, 202, 202);
  box-sizing: border-boxï¼›
}
.burDes .right{
  display: flex
}
.burDes .iconfont{
  font-size: 44rpx;
  color: red;
  margin-top: 4rpx;
}
.buyImg{
  width: 140rpx;
  height: 140rpx;
  background: red;
  border-radius: 10rpx;
  margin-top: -50rpx;
  margin-left: 20rpx;
  margin-right: 40rpx;
  overflow: hidden
}
.buyImg image{
  width: 140rpx;
  height: 140rpx;
}
.buyPrice{
  flex: 1
}
.buyPrice text{
  display: block;
  font-size: 30rpx;
  color: black;
  line-height: 102rpx;
  text-align: left;
  margin-left: 20rpx;
}
.buyPrice text:first-child{
  color: red;
  font-size: 38rpx;
}